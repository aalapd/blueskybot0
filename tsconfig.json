{
    "compilerOptions": {
      "module": "node16",
      "moduleResolution": "node16",
      "target": "es2015",
      "lib": ["es2022"],
      "outDir": "./dist",
      "esModuleInterop": true,
      "strict": true,
      "skipLibCheck": true
    },
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "node_modules",
      "dist"
    ]
  }
  